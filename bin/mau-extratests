#!/bin/bash

INSTANCE="https://openqa.suse.de"
DISTRI="sle"
FLAVOR="Server-DVD-Updates"
TEST="mau-extratests"

# By default list x86_64 architecture
ARGS=$@
if [[ $# -lt 1 ]]; then
	ARGS="--arch x86_64"
fi

openqa-jobs --instance "$INSTANCE" --distri "$DISTRI" --flavor "$FLAVOR" "$TEST" $ARGS
exit $?
